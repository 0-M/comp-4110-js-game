
all: ps3b

ring_buffer_class.o: ring_buffer_class.cpp ring_buffer_class.hpp
	g++ -g -c ring_buffer_class.cpp 

ring_buffer_itr_test.o: ring_buffer_itr_test.cpp
	g++ -g -c ring_buffer_itr_test.cpp

ps3b: ring_buffer_class.o ring_buffer_test.o
	g++ -o ps3b ring_buffer_class.o ring_buffer_itr_test.o -lboost_unit_test_framework

clean:
	$(RM) ps3b ring_buffer_class.o ring_buffer_itr_test.o

