#include <vector>
#include <exception>
#include <stdexcept>
#include <iostream>
#include "guitar_string_class.hpp"

int main(int argc,char* argv[])
{
   std::vector<sf::Int16> v;

  v.push_back(0);
  v.push_back(2000);
  v.push_back(4000);
  v.push_back(-10000);

  GuitarHero::GuitarString gs(v);

	std::cout << gs.sample() << std::endl;
}
