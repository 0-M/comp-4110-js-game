
all: PhotoMagic

lfsr_class.o: lfsr_class.cpp lfsr_class.hpp
	g++ -g -c lfsr_class.cpp 

photo_magic.o: photo_magic.cpp
	g++ -g -c photo_magic.cpp

PhotoMagic: lfsr_class.o photo_magic.o
	g++ -o PhotoMagic lfsr_class.o photo_magic.o -lsfml-graphics -lsfml-window -lsfml-system

clean:
	$(RM) ps2b lfsr_class.o photo_magic.o 

