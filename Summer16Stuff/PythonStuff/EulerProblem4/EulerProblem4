def flipNum(numToFlip,count):
    i = numToFlip
    num2 = 0
    while i != 0:
        num2 += (int(i % 10) * (10 ** count))
        count-= 1
        i //= 10
    return num2

i = 9801
found = False
while found == False:
    num2 = flipNum(i,3)
    if num2 == i:
        print("i: ",i)
        print("num2: ",num2)
        found = True
    else:
        i -= 1
print(num2)

